{
    "extends": [
        "airbnb"
    ],
    "rules": {
        "indent": ["error", 4],
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/jsx-indent" : ["error", 4],
        "react/jsx-indent-props" : ["error", 4],
        "react/jsx-closing-bracket-location" : false,
        "react/jsx-one-expression-per-line": false,
        "react/destructuring-assignment" : false,
        "comma-dangle" : 0,
        "one-var" : 0
    },
    "env":{
        "browser": true
    }
}